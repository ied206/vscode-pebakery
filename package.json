{
    "name": "pebakery",
    "displayName": "PEBakery",
    "description": "PEBakery Syntax Highlighter",
    "version": "1.0.5",
    "publisher": "joveler",
    "license": "MIT",
    "icon": "PEBakeryLogo.png",
    "engines": {
        "vscode": "^1.21.0"
    },
    "keywords": [
        "pebakery",
        "winbuilder"
    ],
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "activationEvents": [
        "*"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/pebakery/vscode-pebakery.git"
    },
    "main": "./out/extension",
    "contributes": {
        "languages": [
            {
                "id": "pebakery",
                "aliases": [
                    "PEBakery",
                    "pebakery"
                ],
                "extensions": [
                    ".script",
                    ".project"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "pebakery",
                "scopeName": "source.pebakery",
                "path": "./syntaxes/PEBakery.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "pebakery",
                "path": "./snippets/PEBakery.snippets.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/node": "^12.12.3",
        "tslint": "^5.20.0",
        "typescript": "^3.6.4",
        "vscode": "^1.1.36"
    }
}
